set old=`pwd`
cd $MOMF_DATAOUT
mkdir REL ABS
echo "Generate file names"
$MOMF_PROG/m03/lib/MAGname
$MOMF_PROG/m03/lib/MAGvariab
cd REL
cp $old/MOMF.ref MOMF.ref
rm -f starfile
ls STAR* > starfile
echo "Start sorting..."
$MOMF_PROG/m03/lib/scattername
echo "Doing relative MAG"
$MOMF_PROG/m03/lib/MAGrelativ
echo "Doing mean MAG"
$MOMF_PROG/m03/lib/MAGmean
echo "Doing new MAG"
$MOMF_PROG/m03/lib/MAGnew
echo "Doing relative MAG"
$MOMF_PROG/m03/lib/MAGrelativ
echo "Doing mean MAG"
$MOMF_PROG/m03/lib/MAGmean
echo "Doing new MAG"
$MOMF_PROG/m03/lib/MAGnew
echo "Doing relative MAG"
$MOMF_PROG/m03/lib/MAGrelativ
echo "Doing mean MAG"
$MOMF_PROG/m03/lib/MAGmean
echo "Doing new MAG"
$MOMF_PROG/m03/lib/MAGnew
echo "Doing relative MAG"
$MOMF_PROG/m03/lib/MAGrelativ
echo "Doing mean MAG"
$MOMF_PROG/m03/lib/MAGmean
echo "Doing new MAG"
$MOMF_PROG/m03/lib/MAGnew
echo "Doing relative MAG"
$MOMF_PROG/m03/lib/MAGrelativ
echo "Doing a sort"
$MOMF_PROG/m03/lib/MAGsort
rm -f scatter3 scatter4 scatter5
sort +2n scatter2 > scatter3 
sort +6n scatter2 > scatter4 
sort +7n scatter2 > scatter5 
echo "Doing another sort"
$MOMF_PROG/m03/lib/MAGsort2
rm -f scatter7
sort +4n scatter6 > scatter7
rm -f MOMF.ref
cd $old
